   May be need support factorialTail was declare after factorial
*/


func factorialTail(n int, acc int) int {
	if n <= 1 {
		return acc
	}
	return factorialTail(n-1, n+acc)
}


func factorial(n int) int {
	return factorialTail(n, 1)
}

func run() int {
    var n = 30000;
    print factorial(n);
    // no return have bug!!!!
    return 0;
}

run();




